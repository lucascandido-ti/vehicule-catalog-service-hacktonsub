    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "vitest run",
    "typecheck": "tsc --noEmit --pretty"
  },
  "dependencies": {
    "@libs/ddd": "workspace:*",
    "@vcs/core": "workspace:*",
    "typeorm": "^0.3.20",
    "zod": "^3.23.8"
  }
}